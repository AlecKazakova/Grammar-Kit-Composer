{
  elementTypeClass="com.example.RootElementType"
  parserUtilClass="com.example.RootParserUtilClass"

  extends="com.intellij.extapi.psi.ASTWrapperPsiElement"

  psiClassPrefix="Root"
  psiImplClassSuffix="Impl"
  psiPackage="com.example.psi"
  psiImplPackage="com.example.psi.impl"
}

dialog ::= (hello_stmt sentences goodbye)*
hello_stmt ::= "Hello" person
person ::= "Alec" | "Philip"
sentences ::= (sentence | extend_stmt)+
sentence ::= "Nice" "work"
goodbye ::= "Goodbye" person

extend_stmt ::= NULL
